<!-- DeviceParts app resources that may need to be customized
     for different hardware or product builds. -->
<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- Path to devices High Brigness Mode toggle file -->
    <string name="pathHBMModeToggle">/sys/devices/platform/soc/ae00000.qcom,mdss_mdp/drm/card0/card0-DSI-1/hbm</string>

    <!-- Path to devices SRGBMode toggle file -->
    <string name="pathSRGBModeToggle">/sys/devices/platform/soc/ae00000.qcom,mdss_mdp/drm/card0/card0-DSI-1/native_display_srgb_color_mode</string>

    <!-- Path to devices DCI-P3 toggle file -->
    <string name="pathDCIModeToggle">/sys/devices/platform/soc/ae00000.qcom,mdss_mdp/drm/card0/card0-DSI-1/native_display_p3_mode</string>

    <!-- Path to devices DCDMode toggle file -->
    <string name="pathDCDModeToggle">/sys/class/drm/card0-DSI-1/dimlayer_bl_en</string>

    <!-- Path to devices WideMode toggle file -->
    <string name="pathWideModeToggle">/sys/devices/platform/soc/ae00000.qcom,mdss_mdp/drm/card0/card0-DSI-1/native_display_wide_color_mode</string>

    <!-- Whether device allow changing calls vibrationlevels -->
    <string name="pathCallVibStrength">/sys/class/leds/vibrator/vmax_mv_call</string>

    <!-- Whether device allow changing notification vibrationlevels -->
    <string name="pathNotifVibStrength">/sys/class/leds/vibrator/vmax_mv_strong</string>

    <!-- Whether device allow changing vibrationlevels -->
    <string name="pathSystemVibStrength">/sys/class/leds/vibrator/vmax_mv_user</string>
    <integer name="vibratorMinMV">116</integer>
    <integer name="vibratorMaxMV">3596</integer>
    <integer name="vibratorDefaultMV">2008</integer>

    <!-- Device Gesture Paths -->
    <string name="pathGestureCircle">/proc/touchpanel/letter_o_enable</string>
    <string name="pathGestureDoubleSwipe">/proc/touchpanel/double_swipe_enable</string>
    <string name="pathGestureDownArrow">/proc/touchpanel/down_arrow_enable</string>
    <string name="pathGestureLeftArrow">/proc/touchpanel/left_arrow_enable</string>
    <string name="pathGestureRightArrow">/proc/touchpanel/right_arrow_enable</string>
    <string name="pathGestureDownSwipe">/proc/touchpanel/down_swipe_enable</string>
    <string name="pathGestureUpSwipe">/proc/touchpanel/up_swipe_enable</string>
    <string name="pathGestureLeftSwipe">/proc/touchpanel/left_swipe_enable</string>
    <string name="pathGestureRightSwipe">/proc/touchpanel/right_swipe_enable</string>
    <string name="pathGestureM">/proc/touchpanel/letter_m_enable</string>
    <string name="pathGestureS">/proc/touchpanel/letter_s_enable</string>
    <string name="pathGestureW">/proc/touchpanel/letter_w_enable</string>

    <!-- Device earpiece gain sysfs -->
    <string name="pathAudioEarpieceGain">/sys/kernel/sound_control/earpiece_gain</string>

    <!-- Device earpiece gain min-max-default values -->
    <integer name="audioEarpieceMin">-10</integer>
    <integer name="audioEarpieceMax">20</integer>
    <integer name="audioEarpieceDefault">0</integer>

    <!-- Device audio mic gain sysfs -->
    <string name="pathAudioMicGain">/sys/kernel/sound_control/mic_gain</string>

    <!-- Device audio mic min-max-default values -->
    <integer name="audioMicGainMin">-10</integer>
    <integer name="audioMicGainMax">20</integer>
    <integer name="audioMicGainDefault">0</integer>

    <!-- Device audio headphone gain sysfs -->
    <string name="pathAudioHeadphoneGain">/sys/kernel/sound_control/headphone_gain</string>

    <!-- Device audio headphone min-max-default values -->
    <integer name="audioHeadphoneGainMin">-60</integer>
    <integer name="audioHeadphoneGainMax">20</integer>
    <integer name="audioHeadphoneGainDefault">0</integer>

    <!-- Device audio speaker gain sysfs -->
    <string name="pathAudioSpeakerGain">/sys/kernel/sound_control/speaker_gain</string>

    <!-- Device audio speaker min-max-default values -->
    <integer name="audioSpeakerGainMin">0</integer>
    <integer name="audioSpeakerGainMax">6</integer>
    <integer name="audioSpeakerGainDefault">3</integer>
</resources>
